n=int(input("enter the number:"))
lst=[]
for i in range(0,n):
      b=int(input("entr the number in list"))
      lst.append(b)
def sumsquare(l):
    odd=0
    even=0
    for i in l:
        if(i%2==0):
            even=even+i**2
        else:
            odd=odd+i**2
    lst=[odd,even]
    return(lst)
print(sumsquare(lst))
